<idea-plugin version="2">
    <name>SpringExt Support</name>
    <description>SpringExt/Webx Support</description>
    <version>1.0</version>
    <vendor>Alibaba.com</vendor>

    <!-- please see http://confluence.jetbrains.net/display/IDEADEV/Build+Number+Ranges for description -->
    <idea-version since-build="123.94" />

    <application-components>
    </application-components>

    <project-components>
        <component>
            <implementation-class>com.alibaba.ide.plugin.intellij.springext.extension.SpringExtFileMonitor</implementation-class>
        </component>
    </project-components>

    <actions>
    </actions>

    <extensions defaultExtensionNs="com.intellij">

        <xml.schemaProvider
                implementation="com.alibaba.ide.plugin.intellij.springext.extension.SpringExtSchemaProvider" />

        <gotoDeclarationHandler
                implementation="com.alibaba.ide.plugin.intellij.springext.extension.SpringExtGotoDeclarationHandler" />

    </extensions>
</idea-plugin>